


def keep_ints(n):
    def print_out(cond):
        i = 0
        while i < n:
            if cond(i) == True:
                print(i)
                i = i + 1
            else:
                i = i +1
    
    return print_out


def is_even(x):
    return x % 2 == 0

def is_above(x):
    return x > 50

def is_odd(x):
    return x % 2 != 0


keep_ints(100)(is_above)